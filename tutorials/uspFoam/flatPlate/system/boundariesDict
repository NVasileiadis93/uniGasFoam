uspPatchBoundaries
(

    boundary
    {
        patchBoundaryProperties
        {
            patch          plate;
        }

        boundaryModel        uspDiffuseWallPatch;

        uspDiffuseWallPatchProperties
        {
            velocity 	    (0 0 0);
            temperature         300;
        }
    }

	boundary
    {
        patchBoundaryProperties
        {
            patch                           inlet;
        }

        boundaryModel   uspDeletionPatch;

        uspDeletionPatchProperties
        {
          allSpecies        yes;
        }
    }

	boundary
    {
        patchBoundaryProperties
        {
            patch                           top;
        }

        boundaryModel   uspDeletionPatch;

        uspDeletionPatchProperties
        {
          allSpecies        yes;
        }
    }

	boundary
    {
        patchBoundaryProperties
        {
            patch                           outlet;
        }

        boundaryModel   uspDeletionPatch;

        uspDeletionPatchProperties
        {
          allSpecies        yes;
        }
    }

);

