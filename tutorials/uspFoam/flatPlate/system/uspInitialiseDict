FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      uspInitialiseDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
configurations
(

    configuration
    {
    
        type                        		uspMeshFill;
        initialDistributionType     		Maxwellian;

        numberDensities{Ar            1.708E+23;};
	translationalTemperature    	300;
	rotationalTemperature      	0;
	vibrationalTemperature     	0;
        electronicTemperature                 	0;
		velocity		        (9.678e+02 0 0);

    }  

);

