FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fieldPropertiesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


uspFields
(
     field
     {
         fieldModel          	uspBinsMethod;
 
         timeProperties
         {
         	timeOption          write;
 	    	resetAtOutput		on;
         }
 
         uspBinsMethodProperties
         {
            field                   N2;
			zone    				channelRegion;
         	typeIds                 (N2);
         	averagingAcrossManyRuns no;
			binModel				uniformBins;
         	
         	uniformBinsProperties
			{
                startPoint	(0 0 0);
			    endPoint	(2e-6 0 0);
			    nBins		60;
			    area		2.64e-14;
			}
         }
     }
     
     field
     {
         fieldModel          	uspVolFields;
 
         timeProperties
         {
         	timeOption          write;
 	    	resetAtOutput		on;
         }
         
         uspVolFieldsProperties
         {
            field                   N2;
         	typeIds                 (N2);
         }
     }

     field
     {
         fieldModel          	uspMassFluxSurface;
 
         timeProperties
         {
         	timeOption          write;
 	    	resetAtOutput		on;
         }
         
         uspMassFluxSurfaceProperties
         {
            field                   N2;
         	typeIds                 (N2);
            faceZone                face1;
            fluxDirection           (1 0 0);
         }
     }

     field
     {
         fieldModel          	uspMassFluxSurface;
 
         timeProperties
         {
         	timeOption          write;
 	    	resetAtOutput		on;
         }
         
         uspMassFluxSurfaceProperties
         {
            field                   N2;
         	typeIds                 (N2);
            faceZone                face2;
            fluxDirection           (1 0 0);
         }
     }
);

