         {
             patch                           wedgeBack;
         }
 
         boundaryModel   uspSpecularWallPatch;
 
         uspSpecularWallPatchProperties
         {
         }
     }


    boundary
    {
        patchBoundaryProperties
        {
            patch                           flow;
        }

        boundaryModel   uspDeletionPatch;

        uspDeletionPatchProperties
        {
            allSpecies      true;
        }
    }

);

