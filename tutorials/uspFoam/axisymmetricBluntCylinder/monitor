set ytics nomirror
set y2tics nomirror

#set xrange[0:5000]      #set the X axes values
#set yrange[0:990000]    #set the Y axes values


#set xtics 1000          #set the sampling value for X axes
#set ytics 1000          #set the sampling value for Y axes


set xlabel "Timestep"
set ylabel "Number of usp particles"
set y2label "Average linear kinetic energy/[J]"

plot \
"< cat log.uspFoam | grep 'Number of USP particles' | cut -d '=' -f 2"  w l axis x1y1 t "Number of usp particles", \
"< cat log.uspFoam | grep 'Average linear kinetic energy' | cut -d '=' -f 2"  w l axis x1y2 t "Average linear kinetic energy"
pause 10
reread 
