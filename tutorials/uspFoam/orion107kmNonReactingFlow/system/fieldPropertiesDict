FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fieldPropertiesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


uspFields
(
     field
     {
         fieldModel          	uspVolFields;
 
         timeProperties
         {
         	timeOption          write;
 	    	resetAtOutput		on;
         }
 
         uspVolFieldsProperties
         {
             field                   N2;
             typeIds                 (N2);
         }
     }

     field
     {
         fieldModel          	uspVolFields;
 
         timeProperties
         {
         	timeOption          write;
 	    	resetAtOutput		on;
         }
 
         uspVolFieldsProperties
         {
             field                   O2;
             typeIds                 (O2);
         }
     }

     
     field
     {
         fieldModel          	uspVolFields;
 
         timeProperties
         {
         	timeOption          write;
 	    	resetAtOutput		on;
         }
 
         uspVolFieldsProperties
         {
             field                   mixture;
             typeIds                 (N2 O2);
         }
     }
);

