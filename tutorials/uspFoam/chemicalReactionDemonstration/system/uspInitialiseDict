FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      uspInitialiseDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
configurations
(
    configuration
    {
        type                    uspMeshFill;

        numberDensities
        {
            N2              1e23;
            O2              1e23;
        };

        translationalTemperature     30000;
        rotationalTemperature     30000;
        vibrationalTemperature     30000;
        electronicTemperature       30000;

        velocity        (0 0 0);
    }
);

